<% layout('layout') %>
<div class="container">
  <div class="row">
    <div class="col-xs-11 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3 col-xs-offset-1">
      <form action="" role="form" method="post">
        <h2>Edit your Stage <small>Make it as simple as possible</small></h2>
        <hr class="colorgraph col-sm-11 col-md-11 col-xs-9">
        <div class="row">
          <div class="col-xs-10 col-sm-12 col-md-12">
            <div class="form-group">
              <input type="text" name="stage" id="stageText" class="form-control input-lg" tabindex="1" value="<%= stageName %>">
            </div>
          </div>
            <div class="col-xs-10 col-sm-6 col-md-6">
              <div class="form-group">
                <input type="submit" name="editTag" value="Submit" class="btn btn-primary btn-block btn-lg" tabindex="3">
              </div>
              </div>
            <div class="col-xs-10 col-sm-6 col-md-6">
              <div class="form-group">
                <a href="/admin/stages" class="btn btn-default btn-block btn-lg">Cancel</a>
              </div>
              </div>
        </form>
      </div>
    </div>
  </div>
</div>
<div class="test">

</div>
